{
  "name": "ec",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --spa",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest"
  },
  "dependencies": {
    "@babel/preset-env": "^7.14.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "2.6.11",
    "firebase": "^8.6.7",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "loader-utils": "^2.0.0",
    "moment": "^2.29.1",
    "nuxt": "^2.15.3",
    "nuxt-typed-vuex": "^0.2.0",
    "ts-loader": "^8.2.0",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@mdi/js": "^5.9.55",
    "@nuxt/types": "^2.15.3",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/jest": "^23.3.10",
    "@vue/test-utils": "^1.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "jest": "^27.0.4",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2",
    "vue-jest": "^3.0.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testURL": "http://localhost/",
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!vue-awesome|vuex-i18n)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  }
}
